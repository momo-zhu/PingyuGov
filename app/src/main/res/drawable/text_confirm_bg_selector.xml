<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/tabLayout_color"/>
            <corners android:radius="5.0dip" />
            <stroke android:color="@color/tabLayout_color"
                android:width="1dp"/>
        </shape>
    </item>

    <item >
        <shape>
            <solid android:color="@color/title_background_color"/>
            <corners android:radius="5.0dip" />
            <stroke android:color="@color/title_background_color"
                android:width="1dp"/>
        </shape>
    </item>


</selector>